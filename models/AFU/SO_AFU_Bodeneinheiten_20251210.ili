    /** Abgeleitet aus BodeneinheitHauptauspraegung. Spezielle Klasse für Waldgebiete. Angereichert mit Attributen, welche nur den Wald betreffen. 
        Die Attribute Skelettgehalt Oberboden und Unterboden kommen erst hier dazu, da Skelettgehalt des Bodens in Landwirtschaft und Wald unterschiedliche Enumeration haben
     */
    CLASS BodeneinheitHauptauspraegung_Landwirtschaft
    EXTENDS BodeneinheitHauptauspraegung =
      /** Nummer der Bodeneinheit
       */
      !!@ ili2db.dispName="Bodeneinheit Nummer"
      Bodeneinheit_Nummer : MANDATORY 1000 .. 9999;
      /** Unterboden 
       */
      !!@ili2db.mapping=EXPAND
      Unterboden : SO_AFU_Bodeneinheiten_20251210.Bodeneinheiten.Unterboden_Landwirtschaft;
      /** Oberboden 
       */
      !!@ili2db.mapping=EXPAND
      Oberboden : SO_AFU_Bodeneinheiten_20251210.Bodeneinheiten.Oberboden_Landwirtschaft;

      !!@ name = "CheckKalkgrenzeKalkgehalt_Land"
      !!@ ilivalid.msg = "Bodeneinheit Hauptausprägung Landwirtschaft: Ungültige Kombination Karbonatgrenze {Karbonatgrenze}, Oberboden Kalkgehalt {Oberboden->Kalkgehalt}." 
      MANDATORY CONSTRAINT ((Karbonatgrenze == 0 AND (Oberboden->Kalkgehalt == #mittel OR Oberboden->Kalkgehalt == #viel OR Oberboden->Kalkgehalt == #wenig))
                          OR (Karbonatgrenze == -1 AND (Oberboden->Kalkgehalt == #kein_kalk OR Oberboden->Kalkgehalt == #nur_im_skelett OR Oberboden->Kalkgehalt == #sehr_wenig OR Oberboden->Kalkgehalt == #wenig))
                          OR (Karbonatgrenze != 0 AND Karbonatgrenze != -1)
                          );
            
      !!@ name = "UntertypENachPH_Unterboden_Land"
      !!@ ilivalid.msg = "Bodeneinheit Hauptausprägung Landwirtschaft: Ph-Wert des Unterboden passt nicht zu Untertyp_E (ist {Untertyp_E})."
      MANDATORY CONSTRAINT (
        NOT (DEFINED(Unterboden->Ph))
        OR (
             (Unterboden->Ph > 6.7  AND Untertyp_E == #E0)
          OR (Unterboden->Ph >= 6.2 AND Unterboden->Ph <= 6.7  AND Untertyp_E == #E1)
          OR (Unterboden->Ph >= 5.1 AND Unterboden->Ph <= 6.1  AND Untertyp_E == #E2)
          OR (Unterboden->Ph >= 4.3 AND Unterboden->Ph <= 5.0  AND Untertyp_E == #E3)
          OR (Unterboden->Ph >= 3.3 AND Unterboden->Ph <= 4.2  AND Untertyp_E == #E4)
          OR (Unterboden->Ph < 3.3  AND Untertyp_E == #E5)
        )
      );  

      !!@ name = "Landwirtschaft_Oberboden_present_if_flag_true"
      !!@ ilivalid.msg = "Bodeneinheit Hauptausprägung Landwirtschaft: Wenn Ohne_Oberboden = TRUE, dann darf kein Oberboden (Tongehalt) erfasst sein."
      MANDATORY CONSTRAINT
        (NOT (Ohne_Oberboden)) OR (NOT (DEFINED(Oberboden->Tongehalt)));

      !!@ name = "Landwirtschaft_Unterboden_present_if_flag_true"
      !!@ ilivalid.msg = "Bodeneinheit Hauptausprägung Landwirtschaft: Wenn Ohne_Unterboden = TRUE, dann darf kein Unterboden (Tongehalt) erfasst sein."
      MANDATORY CONSTRAINT
        (NOT (Ohne_Unterboden)) OR (NOT (DEFINED(Unterboden->Tongehalt)));

      !!@ name = "Landwirtschaft_Oberboden_required_if_flag_false"
      !!@ ilivalid.msg = "Bodeneinheit Hauptausprägung Landwirtschaft: Wenn Ohne_Oberboden = FALSE, muss Oberboden (Tongehalt) erfasst sein."
      MANDATORY CONSTRAINT
        (Ohne_Oberboden) OR (DEFINED(Oberboden->Tongehalt));

      !!@ name = "Landwirtschaft_Unterboden_required_if_flag_false"
      !!@ ilivalid.msg = "Wenn Ohne_Unterboden = FALSE, muss Unterboden (Tongehalt) erfasst sein."
      MANDATORY CONSTRAINT
        (Ohne_Unterboden) OR (DEFINED(Unterboden->Tongehalt));

      !!@ name = "Landwirtschaft_UniqueBodeneinheitProGemeinde"
      !!@ ilivalid.msg = "Bodeneinheit_Nummer {Bodeneinheit_Nummer} ist in Gemeinde_Nr {Gemeinde_Nr} bereits vergeben."
      UNIQUE Gemeinde_Nr, Bodeneinheit_Nummer;

    END BodeneinheitHauptauspraegung_Landwirtschaft;

    ASSOCIATION BodeneinheitHauptauspraegung_Landwirtschaft_Untertyp_K_HauptLandwirtschaft =
      BodeneinheitLandAssociation_K -- {1} BodeneinheitHauptauspraegung_Landwirtschaft;
      Untertyp_K_Association_Land -- {0..10} Untertyp_K_HauptLandwirtschaft;
    END BodeneinheitHauptauspraegung_Landwirtschaft_Untertyp_K_HauptLandwirtschaft;

