      Auch dies, ein «Hack», damit die Kartierer nicht Kataloge etc. importieren und updaten müssen. 
   */
  TOPIC Kartierer =
    /** Die Person(en) welche die Kartierung durchführte. 
     */
    CLASS KartierPerson =
      /** Name der kartierenden Person
       */
      !!@ ili2db.dispName="Nachname"
      Name : MANDATORY TEXT*50;
      /** Vorname der kartierenden person
       */
      !!@ ili2db.dispName="Vorname"
      Vorname : TEXT*50;
    END KartierPerson;

    /** Ein Team besteht aus Kartierern, welche zusammen eine oder mehrere Gebiete Kartieren.
     */
    CLASS KartierTeam =
      /** Jedes Team hat einen Namen bzw. ein Kürzel
       */
      !!@ ili2db.dispName="Team Kürzel"
      TeamKuerzel : MANDATORY TEXT*20;
    END KartierTeam;

    ASSOCIATION KartierPersonTeam =
      KartierPerson_r -- {1 .. *} KartierPerson;
      KartierTeam_r -- {0 .. *} KartierTeam;
    END KartierPersonTeam;

  END Kartierer;

