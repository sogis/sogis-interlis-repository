    END Bodeneinheit_Nebenauspraegungen_Wald;

    CONSTRAINTS OF SO_AFU_Bodeneinheiten_20251210.Bodeneinheiten.Nebenauspraegung_Wald = 
      !!@ name = "HauptHatMehrGewicht_alsNeben_Wald"
      !!@ ilivalid.msg = "Die Hauptausprägung muss mehr Gewichtung haben als die Nebenausprägungen (Wald)."
      MANDATORY CONSTRAINT
        ((Bodeneinheit->Gewichtung) > Gewichtung);
    END;

    CONSTRAINTS OF SO_AFU_Bodeneinheiten_20251210.Bodeneinheiten.BodeneinheitHauptauspraegung_Wald =
      !!@ name = "GewichtungSumme100_Wald" 
      !!@ ilivalid.msg = "Die summierten Gewichtungen (Haupt + Neben) ergeben nicht 100% (Wald)."
      MANDATORY CONSTRAINT 
        (
          DEFINED(Math.sum("Nebenauspraegung->Gewichtung")) AND Math.add(Math.sum("Nebenauspraegung->Gewichtung"), Gewichtung) == 100
        )
        OR 
        (
          Gewichtung == 100 AND NOT(DEFINED(Math.sum("Nebenauspraegung->Gewichtung"))) 
        );
    END;

    CONSTRAINTS OF SO_AFU_Bodeneinheiten_20251210.Bodeneinheiten.BodeneinheitHauptauspraegung_Landwirtschaft =
      !!@ name = "GewichtungSumme100_Land" 
      !!@ ilivalid.msg = "Die summierten Gewichtungen (Haupt + Neben) ergeben nicht 100% (Landwirtschaft)."
      MANDATORY CONSTRAINT 
        (
          DEFINED(Math.sum("Nebenauspraegung->Gewichtung")) AND Math.add(Math.sum("Nebenauspraegung->Gewichtung"), Gewichtung) == 100
        )
        OR 
        (
          Gewichtung == 100 AND NOT(DEFINED(Math.sum("Nebenauspraegung->Gewichtung"))) 
        );
    END;

  END Bodeneinheiten;
