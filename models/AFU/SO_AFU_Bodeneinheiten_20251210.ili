                         OR (Koernungsklasse == #schluffiger_sand  AND (Schluffgehalt >= 15.0 AND Schluffgehalt <= 50.0))
                         OR (Koernungsklasse == #lehmiger_sand  AND (Schluffgehalt >= 0.0  AND Schluffgehalt <= 50.0))
                         OR (Koernungsklasse == #lehmreicher_sand  AND (Schluffgehalt >= 0.0  AND Schluffgehalt <= 50.0))
                         OR (Koernungsklasse == #sandiger_lehm  AND (Schluffgehalt >= 0.0  AND Schluffgehalt <= 50.0))
                         OR (Koernungsklasse == #lehm  AND (Schluffgehalt >= 0.0  AND Schluffgehalt <= 50.0))
                         OR (Koernungsklasse == #toniger_lehm  AND (Schluffgehalt >= 0.0  AND Schluffgehalt <= 50.0))
                         OR (Koernungsklasse == #lehmiger_ton  AND (Schluffgehalt >= 0.0  AND Schluffgehalt <= 50.0))
                         OR (Koernungsklasse == #ton  AND (Schluffgehalt >= 0.0  AND Schluffgehalt <= 50.0))
                         OR (Koernungsklasse == #sandiger_schluff AND (Schluffgehalt >= 50.0 AND Schluffgehalt <= 70.0))
                         OR (Koernungsklasse == #schluff AND (Schluffgehalt >= 70.0 AND Schluffgehalt <= 100.0))
                         OR (Koernungsklasse == #lehmiger_schluff AND (Schluffgehalt >= 50.0 AND Schluffgehalt <= 90.0))
                         OR (Koernungsklasse == #toniger_schluff AND (Schluffgehalt >= 50.0 AND Schluffgehalt <= 70.0))
                         );   
    END Oberboden_Wald;

    /** Eine abstrakte Klasse, welche die meisten Informationen zum Boden enthält. 
     */
    CLASS Auspraegung (ABSTRACT) =
      /** Import OID. Wichtig für den Import aus Excel
       */
      !!@ ili2db.dispName="Import_OID"
      Import_OID : 0 .. 999999999;      
      /** Gewichtung der Ausprägung
       */
      !!@ ili2db.dispName="Gewichtung"
      Gewichtung : MANDATORY 0 .. 100;
      /** Wasserhaushalt. Erfasst wird nur der Code. Weitere Informationen befinden sich in der Klasse Wasserhaushalt.
       */
      !!@ ili2db.dispName="Wasserhaushalt"
      Wasserhaushalt : MANDATORY SO_AFU_Bodeneinheiten_20251210.WasserhaushaltCode_Enum;
      /** Code des Bodentyp
       */
      !!@ ili2db.dispName="Bodentyp"
      Bodentyp : MANDATORY SO_AFU_Bodeneinheiten_20251210.BodentypCode;
      /** Geländeform - Hangneigung und Einschränkungen
       */
      !!@ ili2db.dispName="Geländeform"
      Gelaendeform : MANDATORY SO_AFU_Bodeneinheiten_20251210.Gelaendeform;
      /** Geologie
       */
      !!@ ili2db.dispName="Geologie"
      Geologie : MANDATORY TEXT*30;       
      /** Karbonatgrenze 
       */
      !!@ ili2db.dispName="Karbonatgrenze"
      Karbonatgrenze : MANDATORY -1 .. 1000;        
      /** Mächtigkeit des humosen Oberbodenhorizont (Ah-Horizont)
       */
      !!@ ili2db.dispName="Mächtigkeit Ah-Horizont"
      Maechtigkeit_ah : MANDATORY 0 .. 100;  
      /** Humusgehalt des humosen Oberbodenhorizont (Ah-Horizont)
       */
      !!@ ili2db.dispName="Humusgehalt Ah-Horizont"
      Humusgehalt_ah : MANDATORY 0.0 .. 100.0;        
      /** Bemerkungen
       */
      !!@ ili2db.dispName="Bemerkungen"
      Bemerkungen : TEXT*300;
      /** Bodenprofilwert / Bodenpunktzahl
       */
      !!@ ili2db.dispName="Bodenpunktzahl"
      Bodenpunktzahl : MANDATORY 0 .. 100; 
      /** Pflanzennutzbare Gründigkeit (in cm)
       */
      !!@ ili2db.dispName="Pflanzennutzbare Gründigkeit"
      Pflanzennutzbaregruendigkeit : MANDATORY 0 .. 300;
      /** Untertyp E
       */
      !!@ ili2db.dispName="Untertyp E"
      Untertyp_E : MANDATORY SO_AFU_Bodeneinheiten_20251210.Untertyp_E_Enum;
      /** Untertyp I
       */
      !!@ ili2db.dispName="Untertyp I"
      Untertyp_I : SO_AFU_Bodeneinheiten_20251210.Untertyp_I_Enum;
      /** Untertyp G
       */
      !!@ ili2db.dispName="Untertyp G"
      Untertyp_G : SO_AFU_Bodeneinheiten_20251210.Untertyp_G_Enum;
      /** Untertyp R
       */
      !!@ ili2db.dispName="Untertyp R"
      Untertyp_R : SO_AFU_Bodeneinheiten_20251210.Untertyp_R_Enum;
      
      /** Die Kartierer sollen sich bewusst entscheiden, ob es in dieser Ausprägung einen Oberboden gibt (in diesem Fall sind die Werte des Oberbodens ein zu tragen) oder nicht (in diesem Fall muss dieses Attribut TRUE sein). 
       */
      !!@ ili2db.dispName="Ohne Oberboden"
      Ohne_Oberboden : MANDATORY BOOLEAN;

      /** Die Kartierer sollen sich bewusst entscheiden, ob es in dieser Ausprägung einen Unterboden gibt (in diesem Fall sind die Werte des Unterbodens ein zu tragen) oder nicht (in diesem Fall muss dieses Attribut TRUE sein).
       */ 
      !!@ ili2db.dispName="Ohne Unterboden"
      Ohne_Unterboden : MANDATORY BOOLEAN;

      !!@ name = "CheckWasserhaushaltPflanzennutzbaregruendigkeit"
      !!@ ilivalid.msg = "Ungültige Kombination: Wasserhaushalt {Wasserhaushalt}, Pflanzennutzbare Gründigkeit {Pflanzennutzbaregruendigkeit}."
      MANDATORY CONSTRAINT ((Wasserhaushalt == #a AND Pflanzennutzbaregruendigkeit >= 100) 
                        OR ((Wasserhaushalt == #f OR Wasserhaushalt == #s OR Wasserhaushalt == #k) AND Pflanzennutzbaregruendigkeit >= 70) 
                        OR ((Wasserhaushalt == #b) AND (Pflanzennutzbaregruendigkeit >= 70 AND Pflanzennutzbaregruendigkeit <= 99)) 
                        OR ((Wasserhaushalt == #o) AND (Pflanzennutzbaregruendigkeit >= 50 AND Pflanzennutzbaregruendigkeit <= 100)) 
                        OR ((Wasserhaushalt == #c OR Wasserhaushalt == #g OR Wasserhaushalt == #l OR Wasserhaushalt == #t OR Wasserhaushalt == #v) AND (Pflanzennutzbaregruendigkeit >= 50 AND Pflanzennutzbaregruendigkeit <= 69)) 
                        OR ((Wasserhaushalt == #d OR Wasserhaushalt == #h OR Wasserhaushalt == #m OR Wasserhaushalt == #q OR Wasserhaushalt == #x) AND (Pflanzennutzbaregruendigkeit >= 30 AND Pflanzennutzbaregruendigkeit <= 49)) 
                        OR ((Wasserhaushalt == #p OR Wasserhaushalt == #u OR Wasserhaushalt == #w) AND (Pflanzennutzbaregruendigkeit >= 10 AND Pflanzennutzbaregruendigkeit <= 50)) 
                        OR ((Wasserhaushalt == #e OR Wasserhaushalt == #i OR Wasserhaushalt == #n OR Wasserhaushalt == #r OR Wasserhaushalt == #y) AND Pflanzennutzbaregruendigkeit < 30) 
                        OR (Wasserhaushalt == #z AND Pflanzennutzbaregruendigkeit < 10)
                        );
      !!@ name = "CheckBodentypUntertypen"
      !!@ ilivalid.msg = "Ungültige Kombination: Bodentyp {Bodentyp}, Untertyp_I {Untertyp_I}, Untertyp_G {Untertyp_G}."
      MANDATORY CONSTRAINT ((Bodentyp == #Y AND (Untertyp_I == #I3 OR Untertyp_I == #I4)) 
                         OR (Bodentyp == #I AND Untertyp_I == #I4) 
                         OR (Bodentyp == #V AND (Untertyp_G == #G4 OR Untertyp_G == #G5)) 
                         OR (Bodentyp == #W AND (Untertyp_G == #G5 OR Untertyp_G == #G6)) 
                         OR (Bodentyp == #G AND (Untertyp_G == #G5 OR Untertyp_G == #G6))
                         OR (Bodentyp != #Y AND Bodentyp != #I AND Bodentyp != #V AND Bodentyp != #W AND Bodentyp != #G)
                        );
      !!@ name = "CheckBodentypM_HumusAh"
      !!@ ilivalid.msg = "Ungueltige Kombination: Bei bodentyp M muss Humusgehalt_ah >= 30 sein."                                           
      MANDATORY CONSTRAINT ((Bodentyp != #M) OR (Humusgehalt_ah >= 30));

    END Auspraegung;

