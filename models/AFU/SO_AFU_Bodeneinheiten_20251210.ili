    CONSTRAINTS OF SO_AFU_Bodeneinheiten_20251210.Bodeneinheiten.Nebenauspraegung_Landwirtschaft = 
      !!@ name = "HauptHatMehrGewicht_alsNeben_Landwirtschaft"
      !!@ ilivalid.msg = "Die Hauptausprägung muss mehr Gewichtung haben als die Nebenausprägungen (Landwirtschaft)."
      MANDATORY CONSTRAINT
        ((Bodeneinheit->Gewichtung) > Gewichtung);
    END;

    /** NebenAusprägungen von Boden in Waldgebieten. Abgeleitete aus Auspraegungen. Angereichert mit Attributen, welche nur den Wald betreffen. Ergänzt mit Skelettgehalt.
     */
    CLASS Nebenauspraegung_Wald
    EXTENDS Auspraegung =
      /** Unterboden 
       */
      !!@ili2db.mapping=EXPAND
      Unterboden : SO_AFU_Bodeneinheiten_20251210.Bodeneinheiten.Unterboden_Wald;
      /** Oberboden 
       */
      !!@ili2db.mapping=EXPAND
      Oberboden : SO_AFU_Bodeneinheiten_20251210.Bodeneinheiten.Oberboden_Wald;
      /** Mächtigkeit des Ahh Horizonts
       */    
      !!@ ili2db.dispName="Mächtigkeit Ahh-Horizont"
      Maechtigkeit_ahh : 0.0 .. 20.0;
      /** Humusform im Wald
       */    
      !!@ ili2db.dispName="Humusform Wald"        
      Humusform_wald : MANDATORY SO_AFU_Bodeneinheiten_20251210.Humusform_wald;

      !!@ name = "CheckKalkgrenzeKalkgehalt_Wald_Neben"
      !!@ ilivalid.msg = "Ungueltige Kombination: Karbonatgrenze={Karbonatgrenze}, Oberboden Kalkgehalt={Oberboden->Kalkgehalt}." 
      MANDATORY CONSTRAINT ((Karbonatgrenze == 0 AND (Oberboden->Kalkgehalt == #mittel OR Oberboden->Kalkgehalt == #viel))
                          OR (Karbonatgrenze == -1 AND (Oberboden->Kalkgehalt == #kein_kalk OR Oberboden->Kalkgehalt == #nur_im_skelett OR Oberboden->Kalkgehalt == #sehr_wenig))
                          OR (Karbonatgrenze != 0 AND Karbonatgrenze != -1)
                          );

      !!@ name = "UntertypENachPH_Unterboden_Wald_Neben"
      !!@ ilivalid.msg = "Unterboden pH: Untertyp_E passt nicht (ist {Untertyp_E})."
      MANDATORY CONSTRAINT (
        NOT (DEFINED(Unterboden->Ph))
        OR (
             (Unterboden->Ph > 6.7  AND Untertyp_E == #E0)
          OR (Unterboden->Ph >= 6.2 AND Unterboden->Ph <= 6.7  AND Untertyp_E == #E1)
          OR (Unterboden->Ph >= 5.1 AND Unterboden->Ph <= 6.1  AND Untertyp_E == #E2)
          OR (Unterboden->Ph >= 4.3 AND Unterboden->Ph <= 5.0  AND Untertyp_E == #E3)
          OR (Unterboden->Ph >= 3.3 AND Unterboden->Ph <= 4.2  AND Untertyp_E == #E4)
          OR (Unterboden->Ph < 3.3  AND Untertyp_E == #E5)
        )
      );  

      !!@ name = "Neben_Wald_Oberboden_present_if_flag_true"
      !!@ ilivalid.msg = "Wenn Ohne_Oberboden = TRUE, dann darf kein Oberboden (Tongehalt) erfasst sein."
      MANDATORY CONSTRAINT
        (NOT (Ohne_Oberboden)) OR (NOT (DEFINED(Oberboden->Tongehalt)));

      !!@ name = "Neben_Wald_Unterboden_present_if_flag_true"
      !!@ ilivalid.msg = "Wenn Ohne_Unterboden = TRUE, dann darf kein Unterboden (Tongehalt) erfasst sein."
      MANDATORY CONSTRAINT
        (NOT (Ohne_Unterboden)) OR (NOT (DEFINED(Unterboden->Tongehalt)));

      !!@ name = "Neben_Wald_Oberboden_required_if_flag_false"
      !!@ ilivalid.msg = "Wenn Ohne_Oberboden = FALSE, muss Oberboden (Tongehalt) erfasst sein."
      MANDATORY CONSTRAINT
        (Ohne_Oberboden) OR (DEFINED(Oberboden->Tongehalt));

      !!@ name = "Neben_Wald_Unterboden_required_if_flag_false"
      !!@ ilivalid.msg = "Wenn Ohne_Unterboden = FALSE, muss Unterboden (Tongehalt) erfasst sein."
      MANDATORY CONSTRAINT
        (Ohne_Unterboden) OR (DEFINED(Unterboden->Tongehalt));

    END Nebenauspraegung_Wald;

    ASSOCIATION Nebenauspraegung_Wald_Untertyp_K_NebenWald =
      NebenauspraegungWaldAssociation_K -- {1} Nebenauspraegung_Wald;
      Untertyp_K_Association_Wald -- {0..10} Untertyp_K_NebenWald;
    END Nebenauspraegung_Wald_Untertyp_K_NebenWald;

    ASSOCIATION Nebenauspraegung_Wald_Untertyp_P_NebenWald =
