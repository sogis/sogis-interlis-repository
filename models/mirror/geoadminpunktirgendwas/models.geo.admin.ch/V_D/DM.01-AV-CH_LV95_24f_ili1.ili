      HAli: OPTIONAL HALIGNMENT // indefini = Center //;
      VAli: OPTIONAL VALIGNMENT // indefini = Half //;
      Grandeur: OPTIONAL GrandeurEcriture // indefini = moyenne //;
      Ligne_auxiliaire: OPTIONAL POLYLINE WITH (STRAIGHTS) VERTEX CoordP;
