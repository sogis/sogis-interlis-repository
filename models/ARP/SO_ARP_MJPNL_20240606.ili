      !!@ ili2db.dispName = "Alte Vereinbarungs-Nr (Optional wenn migriert)"
      Vereinbarungs_Nr_Alt : TEXT*50;
      !!@ ili2db.dispName = "Alte Flächen-ID"
      Flaechen_ID_Alt : 0 .. 1000000000;
      Geometrie : MANDATORY GeometryCHLV95_V1.MultiSurface;
      /** Der Link zum Bewirtschafter im GELAN.
       */
      !!@ ili2db.dispName = "Personen ID GELAN"
      GELAN_PID_GELAN : MANDATORY 200000 .. 9999999;
      !!@ ili2db.dispName = "Bewirtschaftungseinheit ID"
      GELAN_BewE_ID : MANDATORY TEXT*100;
      /** Übersteuerung der automatischen Zuteilung des GELAN Bewirtschafters. Default: False.
       */
      !!@ ili2db.dispName = "Übersteuerung GELAN Bewirtschafter"
      Uebersteuerung_Bewirtschafter : MANDATORY BOOLEAN;
      /** Ob der Wechsel der BewE_ID nach GELAN update überprüft wurde, also die Vereinbarung wieder berücksichtigt werden kann.
       */
      !!@ ili2db.dispName = "Bewirtschaftungseinheit überprüft"
      BewE_ID_geprueft : MANDATORY BOOLEAN;
      /** Wird automatisch abgefüllt von Gemeindetabelle. Kann mehrere Gemeinden (BFS-Nr) betreffen (Array of Integer)
       */
      !!@ ili2db.dispName = "BFS-Nr"
      !!@ ili2db.mapping="ARRAY"
      Bfs_Nr : BAG {1..*} OF SO_ARP_MJPNL_20201026.MJPNL.BFS_Nr;
      /** Wird automatisch abgefüllt von Gemeindetabelle. Kann mehrere Gemeinden (Gemeindenamen) betreffen (Array of String)
       */
      !!@ ili2db.dispName = "Gemeinde(n)"
      !!@ ili2db.mapping="ARRAY"
      Gemeinde : BAG {1..*} OF SO_ARP_MJPNL_20201026.MJPNL.String_List;
      /** Wird automatisch abgefüllt von MO-Public Liegenschaften. Kann mehrere Liegenschaften betreffen (Array von String).
       */
      !!@ ili2db.dispName = "GB-Nr"
      !!@ ili2db.mapping="ARRAY"
      GB_Nr : BAG {1..*} OF SO_ARP_MJPNL_20201026.MJPNL.String_List;
      /** Wird automatisch abgefüllt von MO-Public Flurnamen.
       */
      !!@ ili2db.dispName = "Flurname"
      !!@ ili2db.mapping="ARRAY"
      Flurname : BAG {1..*} OF SO_ARP_MJPNL_20201026.MJPNL.String_List;
      /** Wird automatisch abgefüllt mit Kulturflaechen in GELAN
       */
      !!@ ili2db.dispName = "Kultur-Id"
      !!@ ili2db.mapping="ARRAY"
      Kultur_Id : BAG {0..*} OF SO_ARP_MJPNL_20201026.MJPNL.String_List;
      !!@ ili2db.dispName = "Vereinbarungsart"
      Vereinbarungsart : MANDATORY Vereinbarungsart;
      /** Eine Nutzungsvereinbarung ist eine normale Vereinbarung ohne Abgeltungen. Defaultmässig ist es keine Nutzungsvereinbarung.
       */
      !!@ ili2db.dispName = "ist Nutzungsvereinbarung"
      ist_Nutzungsvereinbarung : MANDATORY BOOLEAN;
      /** Fläche in ha
       */
      !!@ ili2db.dispName = "Fläche"
      Flaeche : MANDATORY 0.01 .. 1000000.00 [Units.ha];
      /** Nummer des Regierungsratsbeschlusses
       */
      !!@ ili2db.dispName = "RRB-Nr"
      RRB_Nr : TEXT*50;
      !!@ ili2db.dispName = "RRB Publikationsdatum"
      RRB_publiziert_Ab : INTERLIS.XMLDateTime;
      !!@ ili2db.dispName = "Status der Vereinbarung"
      Status_Vereinbarung : MANDATORY Vereinbarung_Status;
      /** Zur Unterscheidung ob Weide LN/SöG
       * 
       * True: Sömmerungsgebiet
       * False: LN
       */
      !!@ ili2db.dispName = "Landwirtschaftliche Zone"
      Soemmerungsgebiet : MANDATORY BOOLEAN;
      !!@ ili2db.dispName = "MJPNL-Version"
      MJPNL_Version : MANDATORY MJPNL_Version;
      /** in Jahren
       */
      !!@ ili2db.dispName = "Kontrollintervall"
      Kontrollintervall : MANDATORY 1 .. 10;

      !!@ ili2db.dispName = "Laufzeit Startdatum"
      Startdatum : FORMAT INTERLIS.XMLDate "1900-1-1" .. "2100-12-31";
      /** Falls diese Vereinbarung befristet ist (wie bei Buntbrache auf zBs. 8 Jahren) ist das Enddatum das Ende der Laufzeit. Bei allen anderen Vereinbarungen ist es das Datum, auf das gekündigt werden kann (aber nicht muss).
       */
      !!@ ili2db.dispName = "Laufzeit Enddatum (kann je nach dem weiter laufen)"
      Enddatum : FORMAT INTERLIS.XMLDate "1900-1-1" .. "2100-12-31";
      !!@ ili2db.dispName = "Wird kantonsintern abgerechnet"
      Kantonsintern : BOOLEAN;
      !!@ ili2db.dispName = "Bemerkung"
      Bemerkung : MTEXT*1000;
    END Vereinbarung;

    ASSOCIATION AbrechnungLeistung_AbrechnungPerVereinbarung =
      AbrechnungLeistung -- {1..*} Abrechnung_per_Leistung;
      AbrechnungPerVereinbarung -- {0..1} Abrechnung_per_Vereinbarung;
    END AbrechnungLeistung_AbrechnungPerVereinbarung;

    ASSOCIATION AbrechnungPerVereinbarung_AbrechnungPerBewirtschafter =
      AbrechnungPerVereinbarung -- {1..*} Abrechnung_per_Vereinbarung;
      AbrechnungPerBewirtschafter -- {0..1} Abrechnung_per_Bewirtschafter;
    END AbrechnungPerVereinbarung_AbrechnungPerBewirtschafter;

    ASSOCIATION Basisvertrag_Berater =
      Basisvertrag -- {0..*} Basisvertrag;
      Berater -- {0..1} Berater;
    END Basisvertrag_Berater;

    ASSOCIATION Beobachtung_Fauna =
      Beurteilung -- {0..*} Beurteilung;
      Art_Fauna (EXTERNAL) -- {0..*} SO_ARP_MJPNL_20201026.Kataloge.Faunaliste;
      /** optional for now, maybe later mandatory
       */
      !!@ ili2db.dispName = "Häufigkeit (quantitativ)"
      Haeufigkeit_quantitativ : 1 .. 1000;
    END Beobachtung_Fauna;

    ASSOCIATION Beobachtung_Flora =
      Beurteilung -- {0..*} Beurteilung;
      Art_Flora (EXTERNAL) -- {0..*} SO_ARP_MJPNL_20201026.Kataloge.Floraliste;
      !!@ ili2db.dispName = "Häufigkeit (qualitativ)"
      Haeufigkeit_qualitativ : Floraliste_Haeufigkeit;
      !!@ ili2db.dispName = "Häufigkeit (quantitativ)"
      Haeufigkeit_quantitativ : 0 .. 10000;
    END Beobachtung_Flora;

    ASSOCIATION Foto_Beurteilung =
      Foto -- {0..*} Foto;
      Beurteilung -<#> {1} Beurteilung;
    END Foto_Beurteilung;

    /** Zusammenzug von Fauna und Floraliste (für Massnahmen Artenförderung)
     */
    !!@ ili2db.dispName = "Fauna- und Floraliste (kombiniert)"
    VIEW Fauna_Flora_Liste
    UNION OF
      Fa ~ SO_ARP_MJPNL_20201026.Kataloge.Faunaliste,
      Fl ~ SO_ARP_MJPNL_20201026.Kataloge.Floraliste;
    =
      Name_Lateinisch: MANDATORY TEXT*255 := Fa -> Name_Lateinisch,
                                             Fl -> Name_Lateinisch;
      Name_Deutsch: MANDATORY TEXT*255 := Fa -> Name_Deutsch,
                                          Fl -> Name_Deutsch;
      FF_Art: MANDATORY TEXT*50;
    END Fauna_Flora_Liste;

    ASSOCIATION AbrechnungLeistung_Vereinbarung =
      Leistungsabrechnung -- {0..*} Abrechnung_per_Leistung;
      Vereinbarung -- {0..1} Vereinbarung;
    END AbrechnungLeistung_Vereinbarung;

    ASSOCIATION AbrechnungPerVereinbarung_Vereinbarung =
      AbrechnungPerVereinbarung -- {0..*} Abrechnung_per_Vereinbarung;
      Vereinbarung -- {0..1} Vereinbarung;
    END AbrechnungPerVereinbarung_Vereinbarung;

    ASSOCIATION Beurteilung_Vereinbarung =
      Beurteilung -- {0..*} Beurteilung;
      Vereinbarung -- {1} Vereinbarung;
    END Beurteilung_Vereinbarung;

    ASSOCIATION Strukturelement_Flaeche_Vereinbarung =
      Strukturelement_Flaeche -- {0..*} Strukturelement_Flaeche;
      Vereinbarung -<#> {1} Vereinbarung;
    END Strukturelement_Flaeche_Vereinbarung;

    ASSOCIATION Strukturelement_Linie_Vereinbarung =
      Strukturelement_Linie -- {0..*} Strukturelement_Linie;
      Vereinbarung -<#> {1} Vereinbarung;
    END Strukturelement_Linie_Vereinbarung;

    ASSOCIATION Strukturelement_Punkt_Vereinbarung =
      Strukturelement_Punkt -- {0..*} Strukturelement_Punkt;
      Vereinbarung -<#> {1} Vereinbarung;
    END Strukturelement_Punkt_Vereinbarung;

    ASSOCIATION VB_Bewirtschafter_Historie =
      Vereinbarung -<#> {1} Vereinbarung;
      Bewirtschafter_Historie -- {0..*} Vereinbarung_Bewirtschafter_Historie;
    END VB_Bewirtschafter_Historie;

    /** Pro Vereinbarung gibt es nur 1 Subregion. Wenn 2 oder mehr Subregionen betroffen sind, wird die Subregion mit dem grössten Flächenanteil gewählt.
     */
    ASSOCIATION Vereinbarung_UZL_Subregion =
      Vereinbarung -- {0..*} Vereinbarung;
      UZL_Subregion (EXTERNAL) -- {0..1} SO_ARP_MJPNL_20201026.Umweltziele.UZL_Subregion;
    END Vereinbarung_UZL_Subregion;

  END MJPNL;

END SO_ARP_MJPNL_20240606.
